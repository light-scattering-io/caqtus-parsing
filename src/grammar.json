{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "caqtus",
  "rules": {
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "_scalar"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "NAME"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "NAME"
              }
            ]
          }
        }
      ]
    },
    "NAME": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z][a-zA-Z0-9_]*"
      }
    },
    "_scalar": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "quantity"
        }
      ]
    },
    "_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        }
      ]
    },
    "integer": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_SIGN"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "_DIGITS"
          }
        ]
      }
    },
    "float": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_SIGN"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_DIGITS"
                  },
                  {
                    "type": "STRING",
                    "value": "."
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_DIGITS"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "PATTERN",
                            "value": "[eE][+-]?"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_DIGITS"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "_DIGITS"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "STRING",
                    "value": "."
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_DIGITS"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "PATTERN",
                            "value": "[eE][+-]?"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_DIGITS"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_DIGITS"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "PATTERN",
                            "value": "[eE][+-]?"
                          },
                          {
                            "type": "SYMBOL",
                            "name": "_DIGITS"
                          }
                        ]
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "_DIGITS": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[0-9]+_?"
        }
      }
    },
    "_SIGN": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[-+]"
      }
    },
    "quantity": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "magnitude",
          "content": {
            "type": "SYMBOL",
            "name": "float"
          }
        },
        {
          "type": "FIELD",
          "name": "unit",
          "content": {
            "type": "SYMBOL",
            "name": "unit"
          }
        }
      ]
    },
    "unit": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "base",
          "content": {
            "type": "SYMBOL",
            "name": "unit_term"
          }
        },
        {
          "type": "FIELD",
          "name": "multiplicative",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "*"
                },
                {
                  "type": "SYMBOL",
                  "name": "unit_term"
                }
              ]
            }
          }
        },
        {
          "type": "FIELD",
          "name": "divisive",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "/"
                },
                {
                  "type": "SYMBOL",
                  "name": "unit_term"
                }
              ]
            }
          }
        }
      ]
    },
    "unit_term": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "base",
          "content": {
            "type": "SYMBOL",
            "name": "_SINGLE_UNIT"
          }
        },
        {
          "type": "FIELD",
          "name": "exponent",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "^"
                      },
                      {
                        "type": "STRING",
                        "value": "**"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "integer"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "_SINGLE_UNIT": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z]+"
          },
          {
            "type": "STRING",
            "value": "Â°"
          },
          {
            "type": "STRING",
            "value": "%"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
